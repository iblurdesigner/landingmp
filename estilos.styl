test(color=red)
	border solid 1px color
	
@import 'https://fonts.googleapis.com/css?family=Glegoo:400,700';	
/*COLOR NATANJA MEATPRO PARA TEXTO	*/
$colorMP = #F9A81A				
$colorGris = #8c8c8c
$colorCerdo = #ca8f88
$colorRes = #b11f24
	
body
	background-image url('../img/dust_@2x.jpg')
	font-family 'Glegoo'
	font-size 16px
	font-weight 400
	
.rojo
	border solid 1px red

.naranja
	border solid 1px orange

.verde
	border solid 1px green

.rosado
	border solid 1px pink
.blanco
	border solid 2px white	
.celeste
	border solid 1px cyan
.azul
	border solid 1px blue
.alfa
	border solid .1px rgba(0, 0, 0, 0)
.textRes
	color $colorRes
.textCerdo
	color $colorCerdo
.textOran
	color $colorMP
.textGris
	color $colorGris			
.textPeque
	font-size .8rem	
.centrar
	display flex
	flex-wrap wrap
	justify-content center
.centrar2
	display flex
	justify-content space-around	

.sepUp
	margin-top 1rem	
/*.cont90
	width 90%*/
	
/*SUBTITULOS 2 COLOR NARANJA*/
.subti2
	color $colorMP
	font-size 2.5rem
	font-weight 700
	
.subti3
	color $colorMP
	font-size 1.5rem
	font-weight 700
	
.subtiNegro	
	color black
	font-size 3rem
	font-family 'Glegoo'
	font-weight 700
	margin 0
		
.sombraNav
	box-shadow 0 13px 20px 0 rgba(0, 0, 0, 0.5)
	width 100vw
	background-color white
	
.sombra
	box-shadow 0 5px 4px 0 rgba(0, 0, 0, 0.5)

.sombraText
	text-shadow: 0 2px 5px rgba(23, 23, 23, 0.69)
	
.btnEntrar
	border-radius 20px
	background-color $colorMP
	padding .6rem 2.1rem

.navW
	margin-left 1rem
	
/*.navbar-fixed
	margin 0 auto*/

.navmenu
	width 100%
	box-shadow none
	background-color white

.navmenu ul a
	color #a7a7a7
	
.navmenu ul a:hover
	background-color #F9A81A
	color white
.nav-wrapper
	width 80%	
	
.logoNav
	width 189px
	
.logo img
	width 118px
	
.resumen
	font-size 1rem
	font-family 'Glegoo'
	color gray
	line-height normal
		
.fpresentacion
	background-image url('../img/mobile/fondoinicio@2x.jpg')
	background-position center
	background-repeat no-repeat
	background-size cover
	width 100vw
	

/*SECCION CARNIVERY*/	
.carHead
	background-image url('../img/mobile/carHeadMobile@2x.jpg')
	background-position center
	background-repeat no-repeat
	background-size cover
	width 100vw
	height 580px
	
.textCarnivery
	margin-top 280px
	text-align center
	color white	
	
/*SECCION DIA A DIA*/
.madera
	background-image url('../img/mobile/maderaBlanca@2x.jpg')
	background-position center
	background-repeat no-repeat
	background-size cover
	width 100vw
	height auto

.cajaBlanca
	background-color white
	border-radius 20px
.trinche
	height 850px
	background-image url('../img/trinche.png')
	background-repeat no-repeat
	background-size contain
	margin-top 6rem	
		
/*SECCION FIN DE SEMANA*/	
.fondoFinde
	background-image url('../img/mobile/fFinde.jpg')
	background-position center
	background-repeat no-repeat
	background-size cover
	width 100vw
	height 592px
	display flex
	align-items center

.logoFinde
	background-image url('../img/svg/lFinde.svg')
	background-position center, center
	background-repeat no-repeat
	width 80vw
	height 227px
#textFinde
	margin -20px 0px	

#textFinde2
	font-size 1rem	

/*SECCION APP*/
.iphone
	background-image url('../img/mobile/iPhoneCarnivery@2x.png')
	background-repeat no-repeat
	background-size cover
	height 490px
.textApp
	font-size .8rem	
	
/*SECCION INDUSTRY*/
#logoIndustry
	background-image url('../img/logoIndustry.png')
	background-repeat no-repeat
	background-size cover
	height 220px
	width 70%
	margin 0 auto
	
.barraNegra
	background-color black
	color white
	font-size 1.5rem
	text-align center
	line-height 52px
	width 100vw
	height 52px
	
.icoRes
	background-image url('../img/svg/icoRes.svg')
	background-repeat no-repeat
	height 140px
	width 100%
	
.icoCerdo
	background-image url('../img/svg/icoCerdo.svg')
	background-repeat no-repeat
	height 140px
	width 100%
	
.icoPollo
	background-image url('../img/svg/icoPollo.svg')
	background-repeat no-repeat
	height 140px
	width 100%
	
.icoCharcu
	background-image url('../img/svg/icoCharcu.svg')
	background-repeat no-repeat
	height 140px			
	width 100%
	
/*SECCION FOOTER*/
.pieMP
	background-color #e0e0e0 !important

.pieMP a
	color black
	font-size .7rem
	line-height 2rem
.textBold
	font-weight 700	
	font-size 1.2rem !important	
	
/*MOBILE QUERIES*/	
@media screen and (max-width: 760px)
	.nav-wrapper
		width 100%
	.cajaiPhone
		background-color rgba(255, 255, 255, 0.57)
	.pieMP
		font-size 1rem
		line-height 3rem
	#textFinde2 
		display none	

/*TABLET QUERIES*/
@media screen and (min-width: 768px) and (max-width: 991px)
	.nav-wrapper
		width 100%
@media screen and (min-width: 768px)
	.fpresentacion
		background-image url('../img/fInicio.jpg')
	.carHead
		background-image url('../img/fCarnivery.jpg')
	.textCarnivery
		margin-top 400px
	#headDia
		margin-top 5rem
	#logoDia
		padding 0
	.madera
		background-image url('../img/maderaG.jpg')
	.fondoFinde
		background-image url('../img/fondoFinde.jpg')
	.logoFinde
		position absolute
		right 50px
		width 300px
		margin-top -150px	
	#textFinde
		position absolute
		bottom -200px
		right 20px
		width 350px
	.cajaiPhone
		background-color white
		border-radius 1rem	
	.iphone
		background-image url('../img/iPhone.png')
		background-size contain
	#logoIndustry
		background-size cover
		height 300px
		width 50%

/*DESKTOP QUERIES	*/	
@media screen and (min-width: 992px)
	#logoDia
		width 170px			
	.navW
		margin-left 0		
		border solid 1px red
	.sizeAnima
		width 80px
	.cont90
		width 90%
	.logoFinde
		right 150px	
	#logoIndustry
		width 30%
		height 270px